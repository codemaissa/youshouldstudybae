{
  "manifest_version": 3,
  "name": "Yourstudybae",
  "version": "3.0",
  "description": "Study timer for focused girls",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "64": "images/icon64.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "64": "images/icon64.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "64": "images/icon64.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
